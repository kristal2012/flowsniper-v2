{
    "rewrites": [
        {
            "source": "/bybit-api/:match*",
            "destination": "https://api.bybit.com/:match*"
        },
        {
            "source": "/binance-api/:match*",
            "destination": "https://api.binance.com/:match*"
        },
        {
            "source": "/api/:match*",
            "destination": "/api/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}